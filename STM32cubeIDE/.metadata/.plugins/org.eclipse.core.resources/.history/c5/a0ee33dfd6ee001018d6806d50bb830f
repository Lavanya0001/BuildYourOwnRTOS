#include <stdio.h>
#include "led.h"
#include "uart.h"
#include "timebase.h"

static void led_toggle(void){
	led_on();
	delay(2);
	led_off();
	delay(2);
}

static void app(void){
	led_toggle(); //Blue Led toggle/blink
	printf("Hello from STM32WB55\n\r"); //URAT transmission, you need TTL to see this in terminal
}

int main()
{
	led_init();
	uart_tx_init();
	timebase_init();

	while(1)
	{
		app();
	}
	return 0;
}
